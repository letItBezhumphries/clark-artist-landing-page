{
  "name": "clark_artwork",
  "version": "1.0.0",
  "description": "ecommerce application where artists can upload and sell artwork",
  "main": "webpack.config.dev.js",
  "engines": {
    "node": ">=6.13.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/transform-runtime",
      "@babel/plugin-syntax-dynamic-import"
    ]
  },
  "scripts": {
    "watch:sass": "node-sass sass/main.scss public/css/style.css -w",
    "start": "npm-run-all --parallel start-serv react-dev watch:sass",
    "start-serv": "nodemon -r dotenv/config server.js --w",
    "react-dev": "webpack --env.ENVIRONMENT=dev --config webpack.config.dev.js --watch",
    "compile:sass": "node-sass sass/main.scss public/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' public/css/style.comp.css -o public/css/style.prefix.css",
    "compress:css": "node-sass public/css/style.prefix.css public/css/style.css --output-style compressed",
    "build": "npm-run-all compile:sass prefix:css compress:css"
  },
  "author": "Eric Humphries",
  "license": "ISC",
  "dependencies": {
    "@stripe/react-stripe-js": "^1.0.0-beta.3",
    "@stripe/stripe-js": "^1.0.0-beta.4",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6",
    "express-validator": "^6.3.1",
    "gridfs-stream": "^1.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.7",
    "multer": "^1.4.2",
    "multer-gridfs-storage": "^4.0.2",
    "nodemon": "^2.0.2",
    "radium": "^0.26.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-moment": "^0.9.7",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-stripe-elements": "^6.0.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "stripe": "^7.15.0",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "concat": "^1.0.3",
    "core-js": "^3.6.4",
    "css-loader": "^3.4.2",
    "eslint-plugin-react-hooks": "0.0.0-b53ea6ca0",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "postcss-loader": "^3.0.0",
    "regenerator-runtime": "^0.13.3",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.23.1",
    "url-loader": "^2.3.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
}
